{
  "expo": {
    "name": "RideShare",
    "slug": "RideShare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "rideshare",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A1A1A"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#1A1A1A"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
  [
    "expo-router", {"origin": "https://RideShare.com/"}
  ],
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A1A1A"
    }
  ],
  "expo-font"
  ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
